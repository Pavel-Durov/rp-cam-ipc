{
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "python.formatting.autopep8Args": [
    "--indent-size=2"
  ],
  "python.pythonPath": "/usr/local/bin/python3",
  "files.exclude": {
    "**/*.pyc": {
      "when": "$(basename).py"
    },
    "**/__pycache__": true
  },
  "python.formatting.provider": "autopep8"
}